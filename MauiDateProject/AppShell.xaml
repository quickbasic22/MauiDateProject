<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiDateProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiDateProject"
    Shell.FlyoutBehavior="Flyout"
    Title="Shell Content Page">

    <ShellContent
        Title="Week Number Page"
        ContentTemplate="{DataTemplate local:WeekNumberPage}"
        Route="WeekNumberPage" />
    <ShellContent
        Title="Speak day of week in France and Germany"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
